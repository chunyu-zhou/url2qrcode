{
	"name": "url2qrcode",
	"version": "2.1",
	"description": "将当前选项卡的URL转换为二维码，手机可以直接扫码访问。",
	"background": {
        "page": "background.html"
	},
	"browser_action": {
		"default_icon": "icon48.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"permissions": [
		"tabs",
		"http://dwz.cn/"
	],
	"manifest_version": 2
}